{
  "name": "@medusa-clone/data-grid",
  "version": "2.0.0",
  "sideEffects": false,
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "scripts": {
    "build:css": "postcss src/main.css -o dist/styles.css",
    "build:js": "tsup",
    "build": "rm -rf dist && pnpm build:css && pnpm build:js",
    "dev": "tsup --watch",
    "lint": "eslint . --max-warnings 0",
    "clean": "rm -rf .turbo dist"
  },
  "type": "commonjs",
  "devDependencies": {
    "@medusa-clone/typescript-config": "workspace:*",
    "@medusa-clone/ui": "workspace:*",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/lodash.set": "^4.3.9",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "postcss-cli": "^11.0.1",
    "react": "catalog:",
    "tailwindcss": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-hook-form": "^7.0.0"
  },
  "dependencies": {
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.12",
    "lodash.set": "^4.3.2",
    "react-currency-input-field": "^4.0.1",
    "react-hook-form": "^7.63.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
